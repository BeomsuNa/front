{
  "name": "front",
  "private": true,
  "scripts": {
    "build": "turbo run build",

    "dev:frontend": "npm --workspace=apps/photoAi/frontend run dev",
    "dev:backend": "npm --workspace=apps/photoAi/backend run dev",
    "dev": "npm run dev:frontend & npm run dev:backend",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.2",
  "workspaces": [
    "apps/*/frontend",
    "apps/*/backend",
    "packages/*"
  ],
  "dependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "zustand": "^5.0.7"
  }
}
